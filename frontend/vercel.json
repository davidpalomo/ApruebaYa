{
  "name": "vercel",
  "version": "31.0.1",
  "routes": [
    { "src": "/api/(.*)", "dest": "https://apruebaYa-backend.up.railway.app/api/$1" },
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "REACT_APP_API_URL": "https://apruebaYa-backend.up.railway.app/api"
  },
  "github": {
    "enabled": true,
    "silent": false
  }
}
